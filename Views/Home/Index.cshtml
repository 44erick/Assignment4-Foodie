@{
    ViewBag.Title = "Top 5 Restaurants";
}
@model IEnumerable<string>

<div class="text-center">
    <div class="column"></div>
    <!-- List of restaurants-->
    <div class="list-group">
        <ul>
            @foreach (string r in Model)
            {
                <li class="list-group-item">@r</li>
            }
        </ul>
    </div>
    <div class="column"></div>
</div>